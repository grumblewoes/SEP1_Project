<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="view.borrowingProcess.BorrowViewController"
      prefHeight="400.0" prefWidth="600.0">

    <children>
        <HBox alignment="CENTER" prefHeight="49.0" prefWidth="600.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Reservation">
                    <font>
                        <Font name="System Bold" size="22.0" />
                    </font>
                </Text>
            </children>
        </HBox>
        <HBox prefHeight="198.0" prefWidth="600.0">
            <children>
                <VBox alignment="CENTER" prefHeight="100.0" prefWidth="300.0">
                    <children>
                        <HBox alignment="CENTER">
                            <children>
                                <Button mnemonicParsing="false" onAction="#borrowGameButton" text="Borrow" />
                                <Button mnemonicParsing="false" text="Cancel">
                                    <HBox.margin>
                                        <Insets left="30.0" />
                                    </HBox.margin>
                                </Button>
                            </children>
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
                <VBox prefHeight="100.0" prefWidth="300.0">
                    <children>
                        <ScrollPane prefHeight="200.0" prefWidth="200.0">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="239.0">
                                    <children>
                                        <TableView layoutX="-1.0" prefHeight="200.0" prefWidth="240.0">
                                            <columns>
                                                <TableColumn prefWidth="75.0" text="C1" />
                                                <TableColumn prefWidth="75.0" text="C2" />
                                            </columns>
                                        </TableView>
                                    </children>
                                </AnchorPane>
                            </content>
                        </ScrollPane>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</VBox>
